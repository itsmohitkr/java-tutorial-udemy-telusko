<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--beans are the object which are managed by the spring framework-->
    <!-- bean definitions here -->

    <bean id="alien1" class="online.devplanet.Alien" autowire="byName">
        <property name="comp" ref="comp2"/>
    </bean>

    <bean id="comp" class="online.devplanet.Laptop">
    </bean>

    <bean id="comp2" class="online.devplanet.Desktop">
    </bean>

</beans>

